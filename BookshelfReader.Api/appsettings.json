{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "localhost;127.0.0.1",
  "Authentication": {
    "ApiKey": {
      "HeaderName": "X-API-Key",
      "ValidKeys": []
    }
  },
  "OpenLibrary": {
    "BaseUrl": "https://openlibrary.org/"
  },
  "Uploads": {
    "MaxBytes": 10485760,
    "AllowedContentTypes": [
      "image/jpeg",
      "image/png"
    ]
  },
  "Ocr": {
    "Tesseract": {
      "DataPath": "",
      "Language": "eng",
      "MaxDegreeOfParallelism": 2
    }
  },
  "Segmentation": {
    "MinAspectRatio": 0.1,
    "MaxAspectRatio": 10.0,
    "MinAreaFraction": 0.0025,
    "MaxAreaFraction": 0.9,
    "MaxSegments": 64
  },
  "Parsing": {
    "BaseConfidence": 0.35,
    "CommonAuthorTokens": [
      "by",
      "author",
      "edited by"
    ]
  }
}
